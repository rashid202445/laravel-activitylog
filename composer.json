{
    "name": "rashidsiaghi/laravel-activitylog",
    "description": "Custom activity log system based on Spatie Laravel Activitylog",
    "license": "MIT",
    "keywords": [
        "rashidsiaghi",
        "log",
        "user",
        "activity",
        "laravel"
    ],
    "authors": [
        {
            "name": "Rachid Al-Siyaghi",
            "email": "rashid201143@gmail.com",
            "homepage": "https://rashidsiaghi.be",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/rashid202445/activitylog",
    "require": {
        "php": "^8.1",
        "illuminate/config": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/database": "^8.69 || ^9.27 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "spatie/laravel-package-tools": "^1.6.3"
    },
    "require-dev": {
        "ext-json": "*",
        "orchestra/testbench": "^6.23 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "pestphp/pest": "^1.20 || ^2.0 || ^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "RashidSiaghi\\Activitylog\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "RashidSiaghi\\Activitylog\\Test\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RashidSiaghi\\Activitylog\\ActivitylogServiceProvider"
            ]
        },
        "original-package": "spatie/laravel-activitylog",
        "modification-notice": "This package contains modified components from Spatie Laravel Activitylog (MIT Licensed)"
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
